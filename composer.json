{
    "name": "poppy/faker",
    "type": "library",
    "description": "Poppy Faker is a PHP library that generates fake data for zh user.",
    "keywords": [
        "faker",
        "fixtures",
        "data"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Zhao"
        },
        {
            "name": "Fran√ßois Zaninotto"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "ext-intl": "*"
    },
    "autoload": {
        "psr-4": {
            "Poppy\\Faker\\": "src/",
            "Faker\\": "src/Faker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Poppy\\Faker\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
